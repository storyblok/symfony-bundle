{
  "name": "storyblok/symfony-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle for storyblok/php-management-api-client and storyblok/php-content-api-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Silas Joisten",
      "email": "silasjoisten@proton.me"
    }
  ],
  "require": {
    "php": ">=8.3",
    "oskarstark/enum-helper": "^1.6",
    "psr/log": "^3.0",
    "storyblok/php-content-api-client": ">=1.0.0",
    "storyblok/php-tiptap-extension": "^1.0",
    "symfony/config": "^6.0 || ^7.0",
    "symfony/dependency-injection": "^6.0 || ^7.0",
    "symfony/framework-bundle": "^6.0 || ^7.0",
    "symfony/http-client": "^6.0 || ^7.0",
    "symfony/http-kernel": "^6.0 || ^7.0",
    "symfony/monolog-bundle": "^3.10",
    "symfony/string": "^6.0 || ^7.0",
    "thecodingmachine/safe": "^2.0 || ^3.0",
    "twig/twig": "^3.20",
    "webmozart/assert": "^1.11"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.2",
    "ergebnis/data-provider": "^3.2",
    "ergebnis/php-cs-fixer-config": "^6.28",
    "ergebnis/test-util": "^1.5",
    "matthiasnoback/symfony-config-test": "^5.2",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan": "^2.0",
    "phpstan/phpstan-webmozart-assert": "^2.0",
    "phpunit/phpunit": "^10.5",
    "rector/rector": "^2.0",
    "symfony/event-dispatcher": "^6.0 || ^7.1",
    "thecodingmachine/phpstan-safe-rule": "^1.4"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "Storyblok\\Bundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Storyblok\\Bundle\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
